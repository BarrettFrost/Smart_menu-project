type packet struct{
  // 10 - Request RestaurantList(M5);
  // 11 - Return #10;
  // 20 - Request RestaurantUnique(M5);
  // 21 - Return #20;
  // 30 - Register Restaurant, i.e. send RestaurantUnique(DESKTOP);
  // 31 - Return #30, i.e. return RestaurantID;
  RestaurantList[]: *restaurant //List of stored restaurants for m5 search function. ONLY id and names.Used in QueryID #10
  RestaurantUnique: *restaurant //Full file for a single restaurant (ID,name,Menu,MenuSize). Used by QueryID #20 & #30
}

type restaurant struct{
  RestaurantID: string
  RestaurantName: string
  Menu[]: *food
  MenuSize: int
}

type food struct{
  FoodName: string
  Calories: int
  Vegetarian: boolean
  Gluten: boolean
  Nuts: boolean
}
