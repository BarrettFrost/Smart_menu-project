type packet struct{
  QueryID: int // 0:Request restaurants(M5); 1:Return restaurants(WEB); 2:Submit Restaurant(DESKTOP); 3: Return Restaurant ID(WEB)
  Restaurants[]: *restaurant 
  Restaurant: *restaurant // Depreciated once restaurant list implemented
}

type restaurant struct{
  RestaurantID: string
  RestaurantName: string
  Menu[]: *food
  MenuSize: int
}

type food struct{
  FoodName: string
  Calories: int
  Vegetarian: boolean
  Gluten: boolean
  Nuts: boolean
}
